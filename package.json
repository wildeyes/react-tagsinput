{
  "name": "react-tagsinput",
  "version": "3.12.0",
  "description": "Simple react.js component for inputing tags",
  "main": "react-tagsinput.js",
  "peerDependencies": {
    "react": "^15.0.0 || ^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.13.0",
    "isparta": "^4.0.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "react": "15.x.x",
    "react-addons-test-utils": "^15.0.0",
    "react-autosuggest": "^3.7.4",
    "react-dom": "15.x.x",
    "react-input-autosize": "^1.0.0",
    "reactpack": "^0.7.2",
    "standard": "^7.0.1"
  },
  "scripts": {
    "test": "standard src/index.js && mocha --compilers js:babel-register",
    "serve": "browser-sync start --server example --files=\"example/*.*\" --no-open --no-ui --no-ghost-mode",
    "build": "babel --module-id ReactTagsInput -m umd src/index.js > react-tagsinput.js",
    "build-example": "reactpack --no-extract --no-source-map --no-html example/index.js example/",
    "build-example-watch": "reactpack -w --no-extract --no-source-map --no-html example/index.js example/",
    "coverage": "./node_modules/.bin/babel-node node_modules/.bin/isparta cover --report text --report html node_modules/.bin/_mocha -- --reporter dot",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olahol/react-tagsinput"
  },
  "keywords": [
    "react",
    "tags",
    "input",
    "component",
    "javascript",
    "react-component"
  ],
  "author": "Ola Holmstr√∂m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olahol/react-tagsinput/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/olahol/react-tagsinput"
}
